    "import sqlite3\n",
    "import sqlalchemy\n",
    "\n",
    "conn = sqlite3.connect('books.db')\n",
    "curs = conn.cursor()\n",
    "curs.execute('''CREATE TABLE Book (title VARCHAR(20), author VARCHAR(20), year INT)''')\n",
    "\n",
    "select = conn.execute('SELECT title FROM Book ORDER BY title')\n",
    "print(select)"
